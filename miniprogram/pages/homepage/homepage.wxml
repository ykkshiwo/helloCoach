<!--miniprogram/pages/homepage/homepage.wxml-->
<view class="frame">
  <view class="frame-top-part">
    <view class='white-frame'>
      <image src="./logo.png" mode='aspectFit' style="width: 68%" animation="{{lAnimate}}"></image>
    </view>
    <image src="./hellocoach.png" mode='aspectFit' style="height: 26px; width: 70px;"></image>
  </view>
  <!-- <view class='test'></view> -->
  <image src="./loginbutton.png" mode='aspectFit' style="width: 150px;"></image>
  <open-data type="userAvatarUrl"></open-data>
  <open-data class="nick-name" type="userNickName"></open-data>
</view>
